#include <stdio.h>

int sayac_m( );
int dizi1[]={1,3,2,4,52,45,553};
int dizi2[]={3,5,3,4,6,4,5,62,5};

int main()
{
  
   

int sonuc=sayac_m();
printf("%d",sonuc);

    return 0;
}
int sayac_m( )
{
  
  size_t dizi1_uzunlugu = sizeof(dizi1)/sizeof(dizi1[0]);
  size_t dizi2_uzunlugu = sizeof(dizi2)/sizeof(diz2[0]);
  int sayac=0;
  
  for(int i=0;i<dizi1_uzunlugu;i++)
  {
      
     int control=0;
     if(i>0)
     {
         for(int k=0;k<i;k++)
         {
             if(dizi1[i]==dizi1[k])
             {
                  control=1;
                  break;
             }
            
         }
     }
      for(int j=0;j<dizi2_uzunlugu;j++)
      {
          if(control==1)
          break;
          if(dizi1[i]==dizi2[j])
          {
              sayac++;
              break;
              
          }
          
      }
  }
  return sayac;
}
